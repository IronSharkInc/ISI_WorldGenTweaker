<district_spawn_weight>
	<!-- 
		adjust the concentration of the different districts 
		- more downtown
	-->

	<!-- comments on the right indicate the original values -->

	<set xpath="/rwgmixer/district[@name='countrytown']/property[@name='spawn_weight']/@value">0.5</set>
	<set xpath="/rwgmixer/district[@name='countryresidential']/property[@name='spawn_weight']/@value">1</set>
	<set xpath="/rwgmixer/district[@name='commercial']/property[@name='spawn_weight']/@value">0.4</set>
	<set xpath="/rwgmixer/district[@name='industrial']/property[@name='spawn_weight']/@value">0.2</set>
	<set xpath="/rwgmixer/district[@name='residential']/property[@name='spawn_weight']/@value">0.5</set>
	<set xpath="/rwgmixer/district[@name='rural']/property[@name='spawn_weight']/@value">0</set>
	<set xpath="/rwgmixer/district[@name='forest_rural']/property[@name='spawn_weight']/@value">0</set>
	<set xpath="/rwgmixer/district[@name='downtown']/property[@name='spawn_weight']/@value">0.8</set><!-- 0.3 -->
	<set xpath="/rwgmixer/district[@name='oldwest']/property[@name='spawn_weight']/@value">1</set>
	<set xpath="/rwgmixer/district[@name='gateway']/property[@name='spawn_weight']/@value">0</set>
</district_spawn_weight>