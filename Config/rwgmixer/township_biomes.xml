<township>
	<!-- 
		adjust the biomes townships spawn in
		- every biome can spawn every non-biome specific township
	-->
	<set xpath="/rwgmixer/township[@name='oldwest']/property[@name='biomes']/@value">desert</set>
	<set xpath="/rwgmixer/township[@name='forest_countrytown']/property[@name='biomes']/@value">forest</set>
	<set xpath="/rwgmixer/township[@name='bforest_countrytown']/property[@name='biomes']/@value">burntforest</set>
	<set xpath="/rwgmixer/township[@name='bforest_town']/property[@name='biomes']/@value">burntforest</set>
	<set xpath="/rwgmixer/township[@name='countrytown']/property[@name='biomes']/@value">forest,burntforest,desert,snow,wasteland</set><!-- desert,snow -->
	<set xpath="/rwgmixer/township[@name='town']/property[@name='biomes']/@value">forest,burntforest,desert,snow,wasteland</set><!-- desert,snow -->
	<set xpath="/rwgmixer/township[@name='city']/property[@name='biomes']/@value">forest,burntforest,desert,snow,wasteland</set><!-- desert,snow -->
	<set xpath="/rwgmixer/township[@name='wasteland_city']/property[@name='biomes']/@value">wasteland</set>
	<set xpath="/rwgmixer/township[@name='citybig']/property[@name='biomes']/@value">forest,burntforest,desert,snow,wasteland</set><!-- wasteland -->
</township>